<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="CENTER" prefHeight="200.0" prefWidth="300.0"
	minHeight="35.0" spacing="25.0" stylesheets="@../app.css" styleClass="saveFileBox"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
	fx:controller="app.SaveFileController">
	<padding><Insets top="25" right="25" bottom="25" left="25"/></padding>
	<children>
		<Label text="Choose a name..." />
		<TextField fx:id="nameOfFile" prefHeight="30.0" prefWidth="100.0" />
		<Button mnemonicParsing="false" onAction="#handleSave" text="Save" />
	</children>
</VBox>
  